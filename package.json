{
  "name": "posterr-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Guilherme Cunha <guiscunha@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:api": "nodemon --watch src src/interfaces/http/index.ts",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint src --ext .ts,.js",
    "test": "jest --collect-coverage --passWithNoTests --testPathPattern=__tests__/",
    "test:unit": "jest --passWithNoTests --testPathPattern=__tests__/unit",
    "test:integration": "jest --passWithNoTests --testPathPattern=__tests__/instegration",
    "test:e2e": "jest --passWithNoTests --testPathPattern=__tests__/e2e",
    "build": "tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn run typeorm migration:create -d ./src/infrastructure/respositories/typeorm/data-source.ts",
    "migration:generate": "yarn run typeorm migration:generate -d ./src/infrastructure/respositories/typeorm/data-source.ts",
    "migration:up": "yarn run typeorm migration:run -d ./src/infrastructure/respositories/typeorm/data-source.ts",
    "migration:revert": "yarn run typeorm migration:revert -d ./src/infrastructure/respositories/typeorm/data-source.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
